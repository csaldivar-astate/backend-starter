{
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "start": "node --es-module-specifier-resolution=node --no-warning ./dist/index.js",
    "compile": "npm run clean && npx tsc --watch",
    "clean": "rimraf ./dist/",
    "start-dev": "node --es-module-specifier-resolution=node --no-warnings --watch ./dist/index.js",
    "start-dev-browserSync": "node --no-warnings --watch bsync.mjs",
    "start-dev-ui": "concurrently -c cyan,green -n server,browserSync \"npm:start-dev\" \"npm:start-dev-browserSync\"",
    "start-once": "node --es-module-specifier-resolution=node --no-warnings ./dist/index.js",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@eslint/js": "^9.39.2",
    "@types/ejs": "^3.1.5",
    "@types/express-session": "^1.18.2",
    "@types/ip": "^1.1.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.9",
    "@types/nodemailer": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "browser-sync": "^3.0.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.0.0",
    "prettier": "^3.8.0",
    "rimraf": "^6.1.2",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.53.1"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "ejs": "^4.0.1",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "glob": "^13.0.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.12",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.28",
    "zod": "^4.3.5"
  }
}
